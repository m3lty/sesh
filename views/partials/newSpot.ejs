<form action="/spots" enctype="multipart/form-data" method="POST">
    <div class="form-group">
        <label for="spotName">Spot Name</label>
        <input type="text" name="spotName" id="spotName" class="form-control">
        <label for="lat">Lattitude</label>
        <input type="text" name="lat" aria-describedby="locHelp" id="lat" class="form-control">
        <label for="long">Longitude</label>
        <input type="text" name="long" id="long" class="form-control">
        <small id="locHelp" class="form-text text-muted">(Closest physical address to the entrance, try Google Maps!)</small>
        <label for="state">State:</label>

        <input type="file" name="spotImg">
        <button name="button" class="btn btn-primary">Submit</button>
    </div>
</form>