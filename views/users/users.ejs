<% include ../partials/header %>

<% users.forEach(function(users){ %>
  <div class="col-md-3 col-sm-6">
    <div class="thumbnail">
      <img src=" <%= users.avatar %> " alt="user image">
    </div>
    <div class="caption">
      <h4> <%= users.username %> </h4>
    </div>
    <p>
      <a href="/users/<%= users._id %>" class="btn btn-primary">More Info</a>
    </p>
  </div>
<% }); %>



<% include ../partials/footer %>
